{
    "name": "cleentfaar/table-renderer-ascii",
    "type": "library",
    "description": "Extension of the table library that can render your tables in ASCII format",
    "keywords": [
        "cleentfaar",
        "table",
        "ascii"
    ],
    "homepage": "https://github.com/cleentfaar/table-renderer-ascii",
    "license": "MIT",
    "authors": [
        {
            "name": "Cas Leentfaar",
            "email": "info@casleentfaar.com",
            "homepage": "https://github.com/cleentfaar",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.5|^7.0",
        "cleentfaar/table" : "^1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit" : "^4.0|^5.0",
        "scrutinizer/ocular": "^1.1",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "CL\\Table\\Renderer\\": "src/CL/Table/Renderer"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CL\\Table\\Tests\\Renderer\\": "tests/src/CL/Table/Tests/Renderer"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
